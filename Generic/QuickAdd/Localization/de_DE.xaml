<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="LOCQuickAddName">Schnelles Hinzufügen</sys:String>
    <sys:String x:Key="LOCQuickAddAdd">Hinzufügen</sys:String>
    <sys:String x:Key="LOCQuickAddRemove">Entfernen</sys:String>
    <sys:String x:Key="LOCQuickAddToggle">Umschalten</sys:String>

    <sys:String x:Key="LOCQuickAddMenuAdd">{0} hinzufügen</sys:String>
    <sys:String x:Key="LOCQuickAddProgressAdd">Füge {0} hinzu</sys:String>
    <sys:String x:Key="LOCQuickAddSuccessAdd">{0} zu {1} Spielen hinzugefügt</sys:String>

    <sys:String x:Key="LOCQuickAddMenuRemove">{0} entfernen</sys:String>
    <sys:String x:Key="LOCQuickAddProgressRemove">Entferne {0}</sys:String>
    <sys:String x:Key="LOCQuickAddSuccessRemove">{0} aus {1} Spielen entfernt</sys:String>

